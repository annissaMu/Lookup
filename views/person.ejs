<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <title>LookUp</title>
    <!-- load local stylesheet (css) -->
    <link rel="stylesheet" href="/styles.css" />
    <!-- loading jquery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
</head>
<body>
    <% function movieDescription(movie) {
        let m = movie;
        return `${m.title} (${m.release})`;
    }
    %>

    <h1><%= personName %></h1>

    <p><%= description %></p>

    <p>Appeared in:</p>
    <ol>
        <% movieList.forEach( (movie) => {%>
        <!-- how to use ejx in the <a> tag? -->
            
        <a><li><%= movieDescription(movie) %></li></a>
        <% }); %>
    </ol>


    <!-- load local js -->
    <script src="/main.js"></script>
</body>
</html>
